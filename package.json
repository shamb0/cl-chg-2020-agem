{
  "name": "cl-chg-2020-agem",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "files": [
    "build/**/*.js",
    "build/contracts/*",
    "build/artifacts/*json"
  ],
  "scripts": {
    "build": "yarn run build:contracts && yarn run build:typescript && yarn run build:copy",
    "build:contracts": "buidler compile",
    "build:typescript": "tsc -p .",
    "build:copy": "yarn run build:copy:contracts",
    "build:copy:contracts": "copyfiles -u 2 \"contracts/**/*.sol\" \"build/contracts\"",
    "clean": "rm -rf ./artifacts ./build ./cache",
    "test": "yarn run test:contracts",
    "test:contracts": "cross-env SOLPP_FLAGS=\"FLAG_IS_TEST,FLAG_IS_DEBUG\" buidler test --show-stack-traces"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shamb0/cl-chg-2020-agem.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shamb0/cl-chg-2020-agem/issues"
  },
  "homepage": "https://github.com/shamb0/cl-chg-2020-agem#readme",
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.5",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-solpp": "^1.3.3",
    "@nomiclabs/buidler-waffle": "^2.1.0",
    "@nomiclabs/ethereumjs-vm": "^4.2.0",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.10.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^3.0.0",
    "copyfiles": "^2.3.0",
    "cross-env": "^7.0.2",
    "ethereum-waffle": "^3.1.0",
    "ethers": "^5.0.12",
    "glob": "^7.1.6",
    "solidity-coverage": "^0.7.10",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "2.5.1",
    "@types/abstract-leveldown": "^5.0.1",
    "abstract-leveldown": "^6.3.0",
    "bn.js": "4.11.8",
    "debug": "^4.1.1",
    "ethereumjs-util": "^7.0.5"
  }
